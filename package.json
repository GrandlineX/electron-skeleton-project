{
  "name": "@grandlinex/electron-skeleton",
  "version": "0.27.0",
  "description": "electron-skeleton-project",
  "main": "dist/main.js",
  "scripts": {
    "buildprep": "webpack",
    "buildServe": "cross-env BUILD=development npm run buildprep &&  cross-env BUILD=development webpack serve",
    "startDev": "electron-forge start",
    "recompile": "electron-rebuild",
    "lint": "eslint ./src",
    "package": "npm run recompile && cross-env BUILD=production npm run buildprep && electron-forge package",
    "start": "cross-env BUILD=development npm run buildprep && cross-env ELECTRON_IS_DEV=0 electron-forge start"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@grandlinex/e-kernel": "0.28.0",
    "electron": "21.3.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.3",
    "@electron-forge/maker-deb": "^6.0.3",
    "@electron-forge/maker-zip": "^6.0.3",
    "@types/node": "^16.11.14",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "electron": "21.3.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
