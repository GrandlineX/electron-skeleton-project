{
  "name": "@grandlinex/electron-skeleton",
  "version": "0.30.5",
  "description": "electron-skeleton-project",
  "main": "dist/main.js",
  "scripts": {
    "buildprep": "webpack",
    "buildServe": "cross-env BUILD=development npm run buildprep &&  cross-env BUILD=development webpack serve",
    "startDev": "electron-forge start",
    "recompile": "electron-rebuild",
    "lint": "eslint ./src",
    "package": "npm run recompile && cross-env BUILD=production npm run buildprep && electron-forge package",
    "start": "cross-env BUILD=development npm run buildprep && cross-env ELECTRON_IS_DEV=0 electron-forge start"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@grandlinex/e-kernel": "0.30.7",
    "@grandlinex/react-components": "^0.1.3",
    "electron": "24.0.0",
    "electron-devtools-installer": "3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "24.0.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
